{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier", "react"],
  "rules": {
    // To allow prettier errors to be shown by eslint
    "prettier/prettier": "error",

    "no-unused-vars": "warn",
    "no-console": "off",
    "prefer-const": "off",
    "class-methods-use-this": "off"
  }
}
